{
  "name": "IsekaiMusicBot",
  "logo": "https://telegra.ph/file/7ffa8d18b9b7f1b51a81e.jpg",
  "description": "Open-Source bot to play songs in your Telegram's Group Voice Chat. Powered by PyTgCalls.",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/QueenArzoo/VCPlayBot",
  "stack": "container",
  "buildpacks": [
   {
     "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
   }   
  ],        
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram session string",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "Bot Token Dari @BotFather",
      "required": true
    },
    "BOT_USERNAME": {
      "description": "Username Bot Kalian Tanpa @",
      "required": true
    },
    "BOT_NAME": {
      "description": "Nama Bot Music Player Kalian",
      "required": false,
      "value": ""
    },
    "SUPPORT_GROUP": {
      "description": "Masukan Username Group Support Kalian Tanpa @ [Masukan]",
      "required": false,
      "value": "ReszzzUpdate"
    },
    "PROJECT_NAME": {
      "description": "Nama Project MusicPlayer Kalian",
      "required": false,
      "value": "IsekaiBotV1.0"
    },
    "ASSISTANT_NAME": {
      "description": "Nama Music Player Kalian Tanpa @.",
      "required": true
    },
    "ARQ_API_KEY": {
      "description": "Ambil Dari @ARQRobot.",
      "required": false,
      "value": "BVGGFQ-OIAJGY-LLCBTN-ZLCYNG-ARQ"
    },    
    "BG_IMAGE": {
      "description": "BG image of bot (Use transparent one) [IGNORE].",
      "required": false,
      "value": "https://telegra.ph/file/256ba303dbe75754b8506.jpg"
    },     
    "UPDATES_CHANNEL": {
      "description": "Updates Channel Tanpa @ [ReszzzUpdate].",
      "required": false,
      "value": "ReszzzUpdate"
    },    
    "API_ID": {
      "description": "App ID dari my.telegram.org/apps",
      "required": true
    },
    "PMPERMIT": {
      "description": "Ganti Untuk Menonaktifkan",
      "required": false,
      "value": "ENABLE"
    },    
    "API_HASH": {
      "description": "App hash from my.telegram.org/apps",
      "required": true
    },
    "SUDO_USERS": {
      "description": "List of user IDs counted as admin everywhere (separated by space).",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "Max audio duration limit for downloads (minutes).",
      "required": true,
      "value": "15"
    }
  }
}
